{
  "name": "open-music-api-v1",
  "version": "1.0.0",
  "description": "OpenMusic API Version 1",
  "main": "src/server.js",
  "scripts": {
    "start": "nodemon src/server.js",
    "migrate": "node-pg-migrate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "auto-bind": "^4.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "nanoid": "^3.3.4",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^3.1.10"
  },
  "database": {
    "driver": "pg",
    "user": {
      "ENV": "PGUSER"
    },
    "password": {
      "ENV": "PGPASSWORD"
    },
    "host": {
      "ENV": "PGHOST"
    },
    "port": {
      "ENV": "PGPORT"
    },
    "database": {
      "ENV": "PGDATABASE"
    }
  }
}
